{
    "name": "pashkevich/laravel-phone-verification",
    "description": "",
    "keywords": [
        "pashkevich",
        "laravel-phone-verification"
    ],
    "homepage": "https://github.com/siarheipashkevich/laravel-phone-verification",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sergey Pashkevich",
            "email": "siarheipashkveich@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Pashkevich\\PhoneVerification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pashkevich\\PhoneVerification\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pashkevich\\PhoneVerification\\PhoneVerificationServiceProvider"
            ]
        }
    }
}
